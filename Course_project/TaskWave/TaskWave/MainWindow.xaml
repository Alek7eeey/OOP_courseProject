<Window x:Class="TaskWave.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TaskWave"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Title="MainWindow" MinHeight="600" MinWidth="900" MaxHeight="600" MaxWidth="900" Cursor="{StaticResource curSt}" Icon="/logo.png">
    
    <Grid Background="#3F4B60">
        <Border CornerRadius="15">
            <Frame x:Name="mainFrame"  NavigationUIVisibility="Hidden" Margin="35"/>
        </Border>

    </Grid>
</Window>
